<h2>Sign into your account</h2>

<div class="form">

<div class="label">

<%= form_tag login_path do %>

<%= label_tag :email %>
<%= email_field_tag :email %>

<%= label_tag :password %>
<%= password_field_tag :password %>

<%= submit_tag "Sign in" %>

<% if flash[:error_message].present? %>
  <p><%= flash[:error_message] %></p>

</div>
<% end %>

<% end %>

</div>
